{
  "scripts": {
    "watch:postcss": "postcss --watch ./scss/out/*.css --use autoprefixer --use css-mqpacker --use cssnano -d ./static/css",
    "watch:sass": "node-sass --watch --output ./scss/out --source-map true --source-map-contents ./scss/src/index.scss",
    "start": "npm-run-all --parallel watch:**"
  },
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.1.3",
    "bootstrap": "^4.3.1",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  }
}
